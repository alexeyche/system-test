<?xml version='1.0' encoding='UTF-8'?>
<services version='1.0'>
  <admin version='2.0'>
    <adminserver hostalias='node1'/>
    <cluster-controllers>
      <cluster-controller hostalias='node1'/>
      <cluster-controller hostalias='node1'/>
      <cluster-controller hostalias='node1'/>
    </cluster-controllers>
    <slobroks>
      <slobrok hostalias='node1' />
    </slobroks>
  </admin>

  <container version='1.0' id='default'>
    <search/>
    <nodes>
      <node hostalias='node1'/>
    </nodes>
  </container>

  <content version='1.0' id='storage'>
    <redundancy>2</redundancy>
    <group>
      <node hostalias='node1' distribution-key='0'/>
      <node hostalias='node2' distribution-key='1'/>
    </group>
    <tuning>
      <cluster-controller>
        <transition-time>0</transition-time>
      </cluster-controller>
    </tuning>
    <documents>
      <document type='music' mode='store-only'/>
    </documents>
    <engine>
      <proton/>
    </engine>
  </content>

  <container version='1.0' id='doc-api'>
    <document-api>
      <timeout>40</timeout>
    </document-api>
    <http>
      <server id='default' port='19020'/>
    </http>
    <nodes>
      <node hostalias='node1'/>
    </nodes>
  </container>

</services>
